<Application x:Class="ModelGenApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ModelGenApp"
             xmlns:views="clr-namespace:ModelGenApp.Views"
             xmlns:vm="clr-namespace:ModelGenApp.ViewModels"
             StartupUri="MainWindow.xaml">
  <Application.Resources>
    <DataTemplate x:Key="{x:Type local:MainViewModel}" DataType="{x:Type local:MainViewModel}">
      <local:MainWindow/>
    </DataTemplate>
    <DataTemplate x:Key="{x:Type vm:StartProcessViewModel}" DataType="{x:Type vm:StartProcessViewModel}">
      <views:StartWindow/>
    </DataTemplate>
  </Application.Resources>
</Application>
