    </dmw:ParagraphProperties>
    <dmw:Run RsidRunProperties="00D2092D">
      <dmw:RunProperties RunStyle="NazwaProgramowa" />
      <dmw:Text>rId</dmw:Text>
    </dmw:Run>
    <dmw:Run>
      <dmw:RunProperties RunStyle="NazwaProgramowa" />
      <dmw:Text>3</dmw:Text>
    </dmw:Run>
    <dmw:Run>
      <dmw:Text xml:space="preserve"> – relacja do rozszerzonych właściwości dokumentu zapisanych w pliku </dmw:Text>
    </dmw:Run>
    <dmw:Run RsidRunProperties="00D2092D">
      <dmw:RunProperties RunStyle="NazwaProgramowa" />
      <dmw:Text>„</dmw:Text>
    </dmw:Run>
    <dmw:Run>
      <dmw:RunProperties RunStyle="NazwaProgramowa" />
      <dmw:Text>app</dmw:Text>
    </dmw:Run>
    <dmw:Run RsidRunProperties="00D2092D">
      <dmw:RunProperties RunStyle="NazwaProgramowa" />
      <dmw:Text>.xml”</dmw:Text>
    </dmw:Run>
    <dmw:Run>
      <dmw:Text xml:space="preserve"> w folderze </dmw:Text>
    </dmw:Run>
    <dmw:Run RsidRunProperties="00D2092D">
      <dmw:RunProperties RunStyle="NazwaProgramowa" />
      <dmw:Text>„docProps”</dmw:Text>
    </dmw:Run>
    <dmw:Run>
      <dmw:Text>; rozszerzone właściwości dotyczą dokumentów konkretnej aplikacji Office.</dmw:Text>
    </dmw:Run>
  </dmw:Paragraph>
  <dmw:Paragraph ParagraphId="67DA67AF" TextId="77777777" RsidParagraphMarkRevision="00A345CF" RsidParagraphAddition="003D7FF0" RsidParagraphProperties="003D7FF0" RsidRunAdditionDefault="003D7FF0">
    <dmw:ParagraphProperties ParagraphStyleId="Wcicienormalne" />
    <dmw:Run>
      <dmw:Text xml:space="preserve">W każdym folderze podrzędnym może się znajdować podkatalog „_rels” zawierający definicje relacji dla części umieszczonych w tym folderze. Dla przykładu katalog </dmw:Text>
    </dmw:Run>
    <dmw:Run RsidRunProperties="00923F59">
      <dmw:RunProperties RunStyle="NazwaProgramowa" />
      <dmw:Text>„/word”</dmw:Text>
    </dmw:Run>
    <dmw:Run>
      <dmw:Text xml:space="preserve"> zawierający główny plik </dmw:Text>
    </dmw:Run>
    <dmw:Run RsidRunProperties="00923F59">
      <dmw:RunProperties RunStyle="NazwaProgramowa" />
      <dmw:Text>„document.xml”</dmw:Text>
    </dmw:Run>
    <dmw:Run>
      <dmw:Text xml:space="preserve"> ma podkatalog „_rels”, który zawiera plik </dmw:Text>
    </dmw:Run>
    <dmw:Run RsidRunProperties="00923F59">
      <dmw:RunProperties RunStyle="NazwaProgramowa" />
      <dmw:Text>„document.xml.rels”,</dmw:Text>
    </dmw:Run>
    <dmw:Run>
      <dmw:Text xml:space="preserve"> który wiąże plik </dmw:Text>
    </dmw:Run>
    <dmw:Run RsidRunProperties="00923F59">
      <dmw:RunProperties RunStyle="NazwaProgramowa" />
      <dmw:Text>„document.xml</dmw:Text>
    </dmw:Run>
    <dmw:Run>
      <dmw:RunProperties RunStyle="NazwaProgramowa" />
      <dmw:Text>”</dmw:Text>
    </dmw:Run>
    <dmw:Run>
      <dmw:Text xml:space="preserve"> z innymi częściami:</dmw:Text>
    </dmw:Run>
  </dmw:Paragraph>
