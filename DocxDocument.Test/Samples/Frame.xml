      <dmw:Text>„_rels”</dmw:Text>
    </dmw:Run>
    <dmw:Run RsidRunProperties="003B397E">
      <dmw:Text xml:space="preserve"> w katalogu głównym dokumentu zawiera plik (bez nazwy, z samym rozszerzeniem) </dmw:Text>
    </dmw:Run>
    <dmw:Run RsidRunProperties="003B397E">
